{
  "name": "sports_analytics",
  "version": "1.0.0",
  "description": "This project is a sports analytics dashboard for NBA data. It provides detailed player and team statistics, along with interactive filtering and visualization capabilities.",
  "main": "tailwind.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "npx tailwindcss -i ./app/static/css/tailwind.css -o ./app/static/css/output.css",
    "watch:css": "npx tailwindcss -i ./app/static/css/tailwind.css -o ./app/static/css/output.css --watch",
    "build:js": "webpack",
    "watch:js": "webpack --watch",
    "build": "npm run build:css && npm run build:js",
    "watch": "npm run watch:css & npm run watch:js"
  },
  "repository": {
    "type": "git",
